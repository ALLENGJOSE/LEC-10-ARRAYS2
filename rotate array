 public static void rotate(int[] arr, int d) {
    	//Your code goes here
        
        

    
    	
        int n = arr.length;
        int temp[] = new int[d];
        for(int j=0;j<d;j++){
            temp[j] = arr[j];
        }
        int i;
        for(i=0;i<n-d;i++){
            arr[i] = arr[i+d];
        }
        for(int j=0;j<d;j++){
            arr[i] = temp[j];
            i++;
        }
     
 }
